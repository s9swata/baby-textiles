{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["CLERK_SECRET_KEY", "NEON_API_KEY", "RESEND_API_KEY"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": ["CLERK_SECRET_KEY", "NEON_API_KEY", "RESEND_API_KEY"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "clean": {
      "cache": false
    }
  }
}
